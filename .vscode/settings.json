{
  "editor.formatOnSave": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.d.ts, ${capture}.test.ts, ${capture}.test.tsx, ${capture}.*.test.ts, ${capture}.*.test.tsx",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.css": "${capture}.css.map, ${capture}.min.css",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.test.tsx",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, .npmrc, yarn.lock, .yarnrc",
    "vite.config.js": "vite.config.*.js",
    "vite.config.ts": "vite.config.*.ts"
  },
  "files.eol": "\n",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#82a7dd",
    "activityBar.activeBorder": "#f2ccde",
    "activityBar.background": "#82a7dd",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f2ccde",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#82a7dd",
    "statusBar.background": "#3c649f",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#82a7dd",
    "statusBarItem.remoteBackground": "#3c649f",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#3c649f",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#3c649f99",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
