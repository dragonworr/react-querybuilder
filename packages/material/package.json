{
  "name": "@react-querybuilder/material",
  "description": "Custom MUI (Material Design) components for react-querybuilder",
  "version": "6.0.0-alpha.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "index.d.mts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/material"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "yarn typecheck --watch"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "@react-querybuilder/ts": "^6.0.0-alpha.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-querybuilder": "^6.0.0-alpha.0",
    "regenerator-runtime": "^0.13.11",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/icons-material": ">=5.0.0",
    "@mui/material": ">=5.0.0",
    "react": ">=16.8.0"
  }
}
