{
  "name": "@react-querybuilder/chakra",
  "description": "Custom Chakra UI components for react-querybuilder",
  "version": "6.0.0-alpha.1",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "index.d.mts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/chakra"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "yarn typecheck --watch"
  },
  "devDependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.4.9",
    "@chakra-ui/system": "^2.4.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@react-querybuilder/ts": "^6.0.0-alpha.1",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "framer-motion": "^8.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-querybuilder": "^6.0.0-alpha.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "@chakra-ui/icons": ">=1.0.0",
    "@chakra-ui/react": ">=1.6.0",
    "@chakra-ui/system": ">=1.8.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^6.3.3",
    "react": ">=16.8.0"
  }
}
