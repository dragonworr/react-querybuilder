{
  "name": "@react-querybuilder/antd",
  "description": "Custom Ant Design components for react-querybuilder",
  "version": "5.4.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/antd"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "yarn typecheck --watch"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.0",
    "@react-querybuilder/ts": "^5.4.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@vitejs/plugin-react": "^3.0.1",
    "antd": "^5.1.4",
    "dayjs": "^1.11.7",
    "rc-picker": "^3.1.4",
    "rc-select": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-querybuilder": "^5.4.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=4.7.0",
    "antd": ">=4.10.0",
    "react": ">=16.8.0"
  }
